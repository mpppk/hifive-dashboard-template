{
  "name": "hifive-dashboard-template",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "watch": "webpack --watch",
    "test": "nyc mocha",
    "doc": "typedoc --out ./docs -m \"amd\"",
    "start": "webpack-dev-server",
    "lint": "tslint -c tslint.json 'src/**/*.ts' 'test/**/*.ts'",
    "codacycov": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/.bin/codacy-coverage && rm -rf ./coverage",
    "dockerbuild": "docker build -t hifive-dashboard .",
    "dockerrun": "docker run -it -p 8080:8080 hifive-dashboard"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "keywords": [],
  "author": "mpppk",
  "license": "MIT",
  "devDependencies": {
    "@types/chart.js": "^2.6.1",
    "@types/es6-promise": "3.3.0",
    "@types/jest": "^23.3.1",
    "@types/leaflet": "^1.0.67",
    "codecov": "^3.1.0",
    "husky": "^0.14.3",
    "jest": "^23.5.0",
    "prettier": "^1.14.2",
    "pretty-quick": "^1.6.0",
    "source-map-support": "^0.5.0",
    "ts-jest": "^23.1.4",
    "ts-loader": "^3.0.5",
    "tslint": "^5.5.0",
    "tslint-config-prettier": "^1.15.0",
    "typedoc": "^0.10.0",
    "typescript": "^2.4.2",
    "webpack": "^3.3.0",
    "webpack-dev-server": "^3.0.0"
  },
  "dependencies": {
    "chart.js": "^2.6.0",
    "es6-promise": "^4.1.1",
    "jquery": "^3.2.1",
    "leaflet": "^1.1.0",
    "material-design-icons": "^3.0.1"
  }
}
